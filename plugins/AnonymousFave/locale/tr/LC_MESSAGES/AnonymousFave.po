#. TRANS: Client error.
#: actions/anonfavor.php:58
msgid ""
"Could not favor notice! Please make sure your browser has cookies enabled."
msgstr "Bildirim beğenilemedi! Lütfen tarayıcınızın çerezlerinin etkin olduğundan emin olun."

#. TRANS: Client error.
#: actions/anonfavor.php:67
msgid "This notice is already a favorite!"
msgstr "Bu bildirim zaten beğenilen olarak işaretli"

#. TRANS: Server error.
#: actions/anonfavor.php:73
msgid "Could not create favorite."
msgstr "Beğenilen bildirim oluşturulamadı."

#. TRANS: Title.
#: actions/anonfavor.php:82
msgid "Disfavor favorite"
msgstr "Beğendiklerinden çıkar"

#. TRANS: Client error.
#: actions/anondisfavor.php:59
msgid ""
"Could not disfavor notice! Please make sure your browser has cookies "
"enabled."
msgstr "Bildirim beğenilenlerden çıkarılamadı! Lütfen tarayıcınızda çerezlerin etkin olduğundan emin olun."

#. TRANS: Server error.
#: actions/anondisfavor.php:80
msgid "Could not delete favorite."
msgstr "Beğenilen bildirim silinemedi."

#. TRANS: Title.
#: actions/anondisfavor.php:89
msgid "Add to favorites"
msgstr "Beğenilenlere ekle"

#. TRANS: Server exception.
#. TRANS: %d is the notice ID (number).
#: classes/Fave_tally.php:96 classes/Fave_tally.php:125
#, php-format
msgid "Could not update favorite tally for notice ID %d."
msgstr "%d bildirim kimliği için beğenilenler çizelgesi güncellenemedi."

#. TRANS: Server exception.
#. TRANS: %d is the notice ID (number).
#: classes/Fave_tally.php:156
#, php-format
msgid "Could not create favorite tally for notice ID %d."
msgstr "%d bildirim kimliği için beğenilenler çizelgesi oluşturulamadı. "

#. TRANS: Label for tally for number of times a notice was favored.
#: AnonymousFavePlugin.php:165
msgid "Favored"
msgstr "Beğenilen"

#. TRANS: Server exception.
#: AnonymousFavePlugin.php:198 AnonymousFavePlugin.php:209
msgid "Could not create anonymous user session."
msgstr "Adı bilinmeyen kullanıcı oturumu oluşturulamadı."

#. TRANS: Plugin description.
#: AnonymousFavePlugin.php:284
msgid "Allow anonymous users to favorite notices."
msgstr "Adı bilinmeyen kullanıcıların beğenilen bildirimlerine izin ver."
